local cfg = getgenv().config

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local name = player.Name

local Rules = {
	khoemanh = function() loadstring(game:HttpGet("https://draculahub.xyz/get_script"))() end,
	yeukem   = function() repeat wait() until game:IsLoaded() and game.Players.LocalPlayer 
getgenv().Key = "457cc6b7a32e89b9468dce65" 
loadstring(game:HttpGet("https://raw.githubusercontent.com/obiiyeuem/vthangsitink/main/BananaHub.lua"))() end,
}

local function inList(list, value)
	for _, v in ipairs(list) do
		if v == value then return true end
	end
	return false
end

for groupName, list in pairs(cfg.Groups) do
	if inList(list, name) then
		local fn = Rules[groupName]
		if fn then fn() end
		break
	end
end
